{
  "name": "provision-netlify-sites",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "faunadb": "^2.6.0",
    "jsonwebtoken": "^8.4.0",
    "netlify": "^2.2.5",
    "netlify-identity-widget": "^1.5.2",
    "node-fetch": "^2.3.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "simple-oauth2": "^2.2.1"
  },
  "scripts": {
    "fauna-bootstrap": "node ./scripts/fauna-bootstrap.js",
    "fauna-validate": "node ./scripts/fauna-validate.js",
    "start": "npm-run-all --parallel fauna-validate start:app start:server",
    "start:app": "react-scripts start",
    "start:server": "netlify-lambda serve functions",
    "xprebuild": "echo 'setup faunaDB' && npm run fauna-bootstrap",
    "build": "npm-run-all --parallel build:**",
    "build:app": "react-scripts build",
    "build:functions": "netlify-lambda build functions",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "http-proxy-middleware": "^0.19.1",
    "netlify-lambda": "^1.3.0",
    "npm-run-all": "^4.1.5"
  }
}
